#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Reactjs Project/personal/questopia/node_modules/.pnpm/@choojs+findup@0.2.1/node_modules/@choojs/findup/bin/node_modules:/mnt/d/Reactjs Project/personal/questopia/node_modules/.pnpm/@choojs+findup@0.2.1/node_modules/@choojs/findup/node_modules:/mnt/d/Reactjs Project/personal/questopia/node_modules/.pnpm/@choojs+findup@0.2.1/node_modules/@choojs/node_modules:/mnt/d/Reactjs Project/personal/questopia/node_modules/.pnpm/@choojs+findup@0.2.1/node_modules:/mnt/d/Reactjs Project/personal/questopia/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/Reactjs Project/personal/questopia/node_modules/.pnpm/@choojs+findup@0.2.1/node_modules/@choojs/findup/bin/node_modules:/mnt/d/Reactjs Project/personal/questopia/node_modules/.pnpm/@choojs+findup@0.2.1/node_modules/@choojs/findup/node_modules:/mnt/d/Reactjs Project/personal/questopia/node_modules/.pnpm/@choojs+findup@0.2.1/node_modules/@choojs/node_modules:/mnt/d/Reactjs Project/personal/questopia/node_modules/.pnpm/@choojs+findup@0.2.1/node_modules:/mnt/d/Reactjs Project/personal/questopia/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/findup.js" "$@"
else
  exec node  "$basedir/../../bin/findup.js" "$@"
fi
