{
  "name": "vite3-plugin-glslify",
  "version": "0.0.7",
  "description": "Literals collector for Vite fork from vite-plugin-glslify,just update to vite3",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "author": "dairsaber",
  "license": "MIT",
  "scripts": {
    "start": "npm run build -- --watch",
    "build": "tsup src/index.ts --dts --format cjs,esm",
    "prepublishOnly": "npm run build",
    "lint": "eslint --ext .ts ./src",
    "lint:fix": "npm run lint -- --fix",
    "release": "npm run lint && npm run build && release-it"
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "vite": "^3.0.2"
  },
  "dependencies": {
    "@rollup/pluginutils": "^4.2.1",
    "astring": "^1.8.3",
    "glslify": "^7.1.1",
    "magic-string": "^0.26.2",
    "estree-walker": "^2.0.2"
  },
  "devDependencies": {
    "@kuss/eslint-config-vanilla": "^2.6.1",
    "@release-it/conventional-changelog": "^5.0.0",
    "eslint": "^8.20.0",
    "release-it": "^15.1.3",
    "tsup": "^6.1.3",
    "typescript": "^4.7.4",
    "vite": "^3.0.2"
  }
}
